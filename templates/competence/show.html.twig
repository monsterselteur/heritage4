{% extends 'base.html.twig' %}

{% block title %}Competence{% endblock %}

{% block body %}
    <h1>Competence</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ competence.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ competence.nom }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ app.request.headers.get('referer')  }}">Retour</a>

    <a href="{{ path('app_competence_edit', {'id': competence.id}) }}">Modifier</a>

    {{ include('competence/_delete_form.html.twig') }}
{% endblock %}
