{% extends 'base.html.twig' %}
{% block stylesheets %}    <link rel="stylesheet" href="{{ asset('CSS/StyleNew.css') }}"> {% endblock %}
{% block title %}Nouveau groupe compétences{% endblock %}

{% block body %}

    <h1 class="text-center">Créer un nouveau groupe de compétences</h1>
    <div class="formAll">
        {{ form_start(form) }}
        {{ form_row(form.nom) }}
        {{ form_row(form.portefeuille, {'attr': {'class': 'custom-select'}}) }}
        <div class="form-group">
            <button class="btn btn-primary">{{ button_label|default('Save') }}</button>
        </div>
        {{ form_end(form) }}
        <a href="{{app.request.headers.get('referer') }}">Retour</a>
    </div>

{% endblock %}
