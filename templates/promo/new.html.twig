{% extends 'base.html.twig' %}
{% block stylesheets %}    <link rel="stylesheet" href="{{ asset('CSS/StyleNew.css') }}"> {% endblock %}
{% block title %}Nouvelle Promo{% endblock %}

{% block body %}


    <div class="login-card mt-5" >

        <h1 class="text-center">Cr√©er une nouvelle promo</h1>

        <div class="formAll">

            {{ form_start(form) }}
            {{ form_row(form.nom) }}
            {{ form_row(form.date) }}
            <div class="form-group">
                <button class="btn btn-primary">{{ button_label|default('Save') }}</button>
            </div>
            {{ form_end(form) }}
            <a href="{{app.request.headers.get('referer') }}">Retour</a>
        </div>
    </div>
{% endblock %}
